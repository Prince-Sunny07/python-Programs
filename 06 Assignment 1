n=int(input())
num=int(input('any num:'))
largest=num
smallest=num
i=1
while(i<n):
    num=int(input('next any nu:'))
    if(num>largest):
        largest=num
    if(num<smallest):
        smallest=num
    i=i+1
dif=largest-smallest
print(' largest num ',largest)
print('smallest num',smallest)
print('difference=',dif)
